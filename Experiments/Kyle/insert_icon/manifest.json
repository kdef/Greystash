{
    "manifest_version": 2,

    "name": "Password Finder",
    "description": "Finds password fields and grabs text.",
    "version": "0.2",

    "content_scripts": [
      {
        "matches": ["file://*/*"],
        "js": ["findPass.js"]
      }
    ],
    "permissions": [
      "storage"
    ],
    "browser_action": {
        "default_icon": "cat.png"
    }
}